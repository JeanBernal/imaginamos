{
  "name": "create-user",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "build": "echo Compiling *.ts ... && npm run clean && tsc -p .",
    "postbuild": "cp package.json dist/package.json && cd dist && npm install --only=production",
    "clean": "rimraf dist",
    "test-case": "jest -t $1",
    "test": "jest --detectOpenHandles --forceExit",
    "test:coverage": "jest --watchAll false --coverage --detectOpenHandles --forceExit"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ],
    "setupFiles": [
      "<rootDir>/src/test/setEnvVars.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.9.6",
    "@types/validator": "^13.7.1",
    "aws-sdk-client-mock": "^0.5.6",
    "jest": "^27.2.4",
    "sequelize-mock": "^0.10.2",
    "serverless-offline": "^8.2.0",
    "serverless-plugin-typescript": "^2.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescipt": "^1.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.1"
  }
}
